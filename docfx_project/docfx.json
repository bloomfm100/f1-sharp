{
  "metadata": [
    {
      "dest": "api",
      "disableDefaultFilter": false,
      "disableGitFeatures": false,
      "src": [
        {
          "files": [
            "**/*.csproj"
          ],
          "src": "../F1Sharp"
        }
      ]
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "F1Sharp",
      "_appName": "F1Sharp",
      "_enableSearch": true,
      "_disableContribution": true
    },
    "keepFileLink": false,
    "postProcessors": [],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "template": [
      "default",
      "modern"
    ]
  }
}